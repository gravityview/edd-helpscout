{
  "name": "gravityview/github-helpscout",
  "description": "GitHub integration for HelpScout",
  "keywords": ["wordpress", "github", "helpscout"],
  "homepage": "https://wordpress.org/plugins/github-helpscout/",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "Danny van Kooten",
      "email": "hi@dannyvankooten.com",
      "homepage": "https://dannyvankooten.com"
    }
  ],
  "type": "wordpress-plugin",
  "require": {
    "php": ">=5.6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8"
  },
  "autoload": {
    "classmap": [ "includes/" ],
    "files": [ "includes/functions.php" ]
  },
  "scripts": {
    "test": "vendor/bin/phpunit ./tests --bootstrap=tests/bootstrap.php --test-suffix=-test.php"
  }
}
